{
    "environment": "Dev",
    "orchestrator": {
        "publish_adapter": "OutboxWith2PC",
        "application_name": "PythonApplication",
        "base_url": "https://localhost:7055/api",
        "default_callback_webhook": 
        {
            "name": "LocalPythonWebhook",
            "url": "http://127.0.0.1:8000/message/callback",
            "api_token": null
        }
    },    
    "logging": {
        "file_name": "app.log",
        "path": "logs//",
        "level": "trace",
        "max_file_size_in_mb": 50,
        "max_number_of_files": 14
    }
}